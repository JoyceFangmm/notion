{
  "name": "notion",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --host --open",
    "build": "vue-tsc && vite build",
    "lint": "eslint --ext .vue,.js,.ts --fix src",
    "format": "prettier --write src",
    "build_up_dy_qa": "vue-tsc --noEmit --skipLibCheck && vite build --mode qa",
    "build_up_dy_stage": "vue-tsc --noEmit --skipLibCheck && vite build --mode staging",
    "build_up_dy_pro": "vue-tsc --noEmit --skipLibCheck && vite build && ts-node ./scripts/index",
    "build_test": "ts-node ./scripts/index",
    "eslint": "eslint --ext .js,.ts,.vue src",
    "eslint_fix": "eslint --ext .js,.ts,.vue src --fix",
    "prettier": "prettier --write src",
    "preview": "vite preview"
  },
  "dependencies": {
    "@vueuse/core": "^10.7.0",
    "axios": "^1.6.2",
    "element-plus": "^2.6.1",
    "js-cookie": "^3.0.5",
    "jsonp": "^0.2.1",
    "pinia": "^2.1.7",
    "swiper": "^11.0.5",
    "vue": "^3.3.4",
    "vue-clipboard3": "^2.0.0",
    "vue-lazyload": "^3.0.0",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.7.2",
    "@types/js-cookie": "^3.0.6",
    "@types/jsonp": "^0.2.3",
    "@types/node": "^20.11.20",
    "@vitejs/plugin-legacy": "^4.1.1",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "archiver": "^6.0.1",
    "autoprefixer": "^10.4.16",
    "consola": "^3.2.3",
    "eslint": "^8.57.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "eslint-plugin-vue": "^9.22.0",
    "gh-pages": "^6.1.1",
    "postcss-px-to-viewport-8-plugin": "^1.2.3",
    "prettier": "3.2.5",
    "request": "^2.88.2",
    "rollup-plugin-esbuild": "^6.1.1",
    "sass": "^1.69.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.0.2",
    "unplugin-auto-import": "^0.17.5",
    "unplugin-vue-components": "^0.26.0",
    "unplugin-vue-markdown": "^0.26.0",
    "vite": "^4.4.5",
    "vite-plugin-style-import": "^2.0.0",
    "vue-tsc": "^1.8.5"
  }
}
